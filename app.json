{
  "expo": {
    "name": "shelfCheck",
    "slug": "shelfCheck",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.shelfcheck.shelfcheck",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "shelfCheck needs your location to find stores near you for the items you need"
      }
    },
    "android": {
      "package": "com.shelfcheck.shelfcheck",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
